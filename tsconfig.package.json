{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "Node",
    "target": "es2020",
    "sourceMap": true,
    "strict": true,
    "incremental": true,
    "declaration": true,
    "jsx": "react",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "outDir": "./dist"
  },
  "include": ["typechain-types/**/*.ts"]
}
